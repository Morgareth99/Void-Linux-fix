{
    "perl"        : "6.*",
    "name"        : "Voidvault",
    "version"     : "1.16.0",
    "author"      : "atweiden",
    "description" : "Bootstrap Void with FDE",
    "license"     : "Unlicense",
    "source-type" : "git",
    "source-url"  : "git://github.com/atweiden/voidvault.git",
    "support"     : {
        "bugtracker" : "https://github.com/atweiden/voidvault/issues",
        "source"     : "https://github.com/atweiden/voidvault"
    },
    "provides" : {
        "Crypt::Libcrypt"                   : "lib/Crypt/Libcrypt.pm6",
        "Crypt::Libcrypt::Glibc"            : "lib/Crypt/Libcrypt/Glibc.pm6",
        "Crypt::Libcrypt::Musl"             : "lib/Crypt/Libcrypt/Musl.pm6",
        "Void::Utils"                       : "lib/Void/Utils.pm6",
        "Void::XBPS"                        : "lib/Void/XBPS.pm6",
        "Voidvault"                         : "lib/Voidvault.pm6",
        "Voidvault::Base"                   : "lib/Voidvault/Base.pm6",
        "Voidvault::Config"                 : "lib/Voidvault/Config.pm6",
        "Voidvault::Config::Base"           : "lib/Voidvault/Config/Base.pm6",
        "Voidvault::Config::OneFA"          : "lib/Voidvault/Config/OneFA.pm6",
        "Voidvault::Constants"              : "lib/Voidvault/Constants.pm6",
        "Voidvault::Grammar"                : "lib/Voidvault/Grammar.pm6",
        "Voidvault::OneFA"                  : "lib/Voidvault/OneFA.pm6",
        "Voidvault::Replace"                : "lib/Voidvault/Replace.pm6",
        "Voidvault::Replace::Crypttab"      : "lib/Voidvault/Replace/Crypttab.pm6",
        "Voidvault::Replace::DNSCryptProxy" : "lib/Voidvault/Replace/DNSCryptProxy.pm6",
        "Voidvault::Replace::Dhcpcd"        : "lib/Voidvault/Replace/Dhcpcd.pm6",
        "Voidvault::Replace::Dracut"        : "lib/Voidvault/Replace/Dracut.pm6",
        "Voidvault::Replace::FSTab"         : "lib/Voidvault/Replace/FSTab.pm6",
        "Voidvault::Replace::GRUB::Default" : "lib/Voidvault/Replace/GRUB/Default.pm6",
        "Voidvault::Replace::GRUB::Linux"   : "lib/Voidvault/Replace/GRUB/Linux.pm6",
        "Voidvault::Replace::Hosts"         : "lib/Voidvault/Replace/Hosts.pm6",
        "Voidvault::Replace::Locales"       : "lib/Voidvault/Replace/Locales.pm6",
        "Voidvault::Replace::OpenResolv"    : "lib/Voidvault/Replace/OpenResolv.pm6",
        "Voidvault::Replace::PAM"           : "lib/Voidvault/Replace/PAM.pm6",
        "Voidvault::Replace::RC"            : "lib/Voidvault/Replace/RC.pm6",
        "Voidvault::Replace::SSH::Moduli"   : "lib/Voidvault/Replace/SSH/Moduli.pm6",
        "Voidvault::Replace::SSH::SSHD"     : "lib/Voidvault/Replace/SSH/SSHD.pm6",
        "Voidvault::Replace::SecureTTY"     : "lib/Voidvault/Replace/SecureTTY.pm6",
        "Voidvault::Replace::Sudoers"       : "lib/Voidvault/Replace/Sudoers.pm6",
        "Voidvault::Replace::Sysctl"        : "lib/Voidvault/Replace/Sysctl.pm6",
        "Voidvault::Types"                  : "lib/Voidvault/Types.pm6",
        "Voidvault::Utils"                  : "lib/Voidvault/Utils.pm6",
        "X::Void::XBPS"                     : "lib/X/Void/XBPS.pm6"
    },
    "resources" : [
        "etc/X11/Xwrapper.config",
        "etc/X11/xorg.conf.d/10-synaptics.conf",
        "etc/X11/xorg.conf.d/99-security.conf",
        "etc/hosts",
        "etc/modprobe.d/modprobe.conf",
        "etc/modules-load.d/bbr.conf",
        "etc/nftables.conf",
        "etc/nftables/wireguard/table/inet/filter/forward/wireguard.nft",
        "etc/nftables/wireguard/table/inet/filter/input/wireguard.nft",
        "etc/nftables/wireguard/table/wireguard.nft",
        "etc/profile.d/shell-timeout.sh",
        "etc/resolvconf.conf",
        "etc/securetty",
        "etc/ssh/ssh_config",
        "etc/ssh/sshd_config",
        "etc/sysctl.d/99-sysctl.conf",
        "etc/udev/rules.d/60-io-schedulers.rules",
        "usr/bin/genfstab"
    ]
}
